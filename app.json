{
  "name": "SARKAR-AK",
  "version": "3.0.0",
  "description": "Sarkar-AK Simple WhatsApp Bot",
  "logo": "https://github.com/khanak1/zimbot-v4/blob/zim-bot/c57517af20709107a09ea46938e21000.png",
  "keywords": ["whatsapp", "bot", "ak", "multi-device"],
  "repository": "https://github.com/A-S-W-I-N-S-P-A-R-K-Y/X-BOT-MD",
  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "id",
      "required": true,
      "value": ""
    },
    "HANDLER": {
      "description": "Enter Prefix For Commands",
      "required": true,
      "value": "."
    },
    "SUDO": {
      "description": "Enter Numbers With Country Code Without + Separated With , To Add More Numbers",
      "required": true,
      "value": "923556159234"
    },
    "HEROKU_APP_NAME": {
      "description": "Enter Your Heroku App Name Same As Above You Entered",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Enter Your Heroku Account Api Key. Get It From https://dashboard.heroku.com/account",
      "required": true
    },
    "STICKER_DATA": {
      "description": "Enter Your Sticker PackName And Author Name",
      "required": true,
      "value": "Sarkar-Ak;AFZAAL KHAN"
    },
    "AUDIO_DATA": {
      "description": "Enter Your Owner Name",
      "required": true,
      "value": "Sarkar-Ak;AFZAAL KHAN;https://github.com/khanak1/zimbot-v4/blob/zim-bot/c57517af20709107a09ea46938e21000.png"
    },
    "BOT_INFO": {
      "description": "Enter Your Owner Number",
      "required": true,
      "value": "923556159234"
    },
    "ALWAYS_ONLINE": {
      "description": "Enter Your Bot Name",
      "required": false,
      "value": "Sarkar-Ak"
    },
    "WORK_TYPE": {
      "description": "Enter Your Mode Type public For Public Otherwise Type private",
      "required": false,
      "value": "public"
    },
    "AUTO_STATUS_VIEW": {
      "description": "If You Want To View Status Automatically, Type true. Otherwise, Type false",
      "required": true,
      "value": "true"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    }
  ]
}
